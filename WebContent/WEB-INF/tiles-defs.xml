<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>

	<!-- TEMPLATE -->
	<definition name="templateDefault"
		template="/WEB-INF/tiles/templateDefault.jsp">
		<put-attribute name="header"
			value="/WEB-INF/tiles/header.jsp" />
		<put-attribute name="navbar"
			value="/WEB-INF/tiles/navbar.jsp" />
		<put-attribute name="main"
			value="/WEB-INF/tiles/body.jsp" />
		<put-attribute name="footer"
			value="/WEB-INF/tiles/footer.jsp" />
	</definition>

	<definition name="templateAjax"
		template="/WEB-INF/tiles/templateAjax.jsp">
		<put-attribute name="body"
			value="/WEB-INF/tiles/body.jsp" />
		<put-attribute name="form"
			value="/WEB-INF/tiles/body.jsp" />
		<put-attribute name="lista"
			value="/WEB-INF/tiles/body.jsp" />
	</definition>


	<definition name="listaJSP" extends="templateDefault">
		<put-attribute name="main"
			value="/WEB-INF/jsp/playgroundLista.jsp" />
	</definition>

</tiles-definitions>